{
  "name": "minute_taker",
  "version": "1.0.0",
  "description": "Allows users to take and distribute meeting notes in teams.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "node seeders/seeds.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cody-callahan/minute_taker.git"
  },
